   ],
   "source": [
    "\n",
    "x = 1\n",
    "for i in range(2,101):\n",
    "    x *= i\n",
    "    \n",
    "sum_ = 0\n",
    "while x>0:\n",
    "    sum_ += x % 10\n",
    "    x //= 10\n",
    "    \n",
    "digits = [int(c) for c in str(x)]\n",
    "sum_ = sum(digits)\n",
    "print(sum_)\n"
   ]
